    "@fontsource/source-sans-pro": "^5.0.8",
    "astro": "^3.1.0",
    "feather-icons": "^4.29.1",
    "findup-sync": "^5.0.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^12.0.1",
    "levenshtein": "^1.0.5",
    "marked": "^9.0.2",
    "marked-smartypants": "^1.1.3",
    "memize": "^2.1.0",
    "node-fetch-cache": "^3.1.3",
    "p-limit": "^4.0.0",
